{
    "name": "axstrad/symfony-extra",
    "description": "Convenience abstract class and traits when using Symonfy's Components",
    "type": "library",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Kempster",
            "email": "dev@dankempster.co.uk",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.6 || ~3.0"
    },

    "require-dev": {
        "axstrad/test": "~0.1.0",
        "phpunit/phpunit": "~4.1@stable"
    },

    "autoload": {
        "psr-4": {
            "Axstrad\\Symfony\\": ""
        }
    },

    "minimum-stability": "dev",

    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "2.0.x-dev"
        }
    },

    "archive": {
        "exclude": ["!vendor", "Tests", "phpunit.xml*", ".gitignore", ".travis.yml"]
    }
}
